{
	"name": "sveltetodo",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"predev": "npm run prisma:migrate:dev",
		"dev": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview",
		"postinstall": "prisma generate",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint:fix": "prettier --plugin-search-dir . --check . && eslint .",
		"lint:format": "prettier --config prettier.config.cjs --write --plugin-search-dir=. .",
		"lint": "npm run lint:format && npm run lint:fix",
		"storybook": "start-storybook -p 6006 --no-open",
		"build-storybook": "build-storybook",
		"prisma:migrate:dev": "prisma migrate dev",
		"prisma:migrate:reset": "prisma migrate reset --force",
		"test": "jest",
		"test:watch": "npm test -- --watch"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.20.2",
		"@prisma/client": "^4.7.1",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "1.0.0-next.577",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/svelte": "^3.2.2",
		"@trivago/prettier-plugin-sort-imports": "^4.0.0",
		"@trpc/client": "^10.4.3",
		"@trpc/server": "^10.4.3",
		"@types/jest": "^29.2.4",
		"@types/sortablejs": "^1.15.0",
		"@typescript-eslint/eslint-plugin": "^5.46.0",
		"@typescript-eslint/parser": "^5.46.0",
		"autoprefixer": "^10.4.13",
		"babel-jest": "^29.3.1",
		"eslint": "^8.29.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"jest": "^29.3.1",
		"lucide-svelte": "^0.104.0",
		"postcss": "^8.4.19",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.1",
		"prettier-plugin-svelte": "^2.8.1",
		"prettier-plugin-tailwindcss": "^0.2.1",
		"prisma": "^4.7.1",
		"svelte": "^3.54.0",
		"svelte-check": "^2.10.2",
		"svelte-dnd-action": "0.9.17",
		"svelte-jester": "^2.3.2",
		"svelte-loader": "^3.1.4",
		"svelte-popperjs": "^1.3.2",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "3.2.4",
		"trpc-sveltekit": "^3.0.2",
		"ts-jest": "^29.0.3",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.4",
		"vite": "^3.2.0",
		"zod": "^3.19.1",
		"@tailwindcss/forms": "^0.5.3"
	},
	"type": "module"
}