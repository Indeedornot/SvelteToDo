{
	"name": "sveltetodo",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"predev": "npm run prisma:migrate:dev",
		"dev": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview",
		"postinstall": "prisma generate",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint:fix": "prettier --plugin-search-dir . --check . && eslint .",
		"lint:format": "prettier --config prettier.config.cjs --write --plugin-search-dir=. .",
		"lint": "npm run lint:format && npm run lint:fix",
		"storybook": "start-storybook -p 6006 --no-open",
		"build-storybook": "build-storybook",
		"prisma:migrate:dev": "prisma migrate dev",
		"prisma:migrate:reset": "prisma migrate reset --force",
		"test": "jest",
		"test:watch": "npm test -- --watch"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.20.2",
		"@popperjs/core": "^2.11.6",
		"@prisma/client": "^4.6.1",
		"@storybook/addon-actions": "^6.5.13",
		"@storybook/addon-essentials": "^6.5.13",
		"@storybook/addon-interactions": "^6.5.13",
		"@storybook/addon-links": "^6.5.13",
		"@storybook/addon-svelte-csf": "^2.0.10",
		"@storybook/builder-vite": "^0.2.5",
		"@storybook/svelte": "^6.5.13",
		"@storybook/testing-library": "^0.0.13",
		"@storybook/theming": "^6.5.13",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/svelte": "^3.2.2",
		"@trivago/prettier-plugin-sort-imports": "^3.4.0",
		"@types/jest": "^29.2.3",
		"@types/sortablejs": "^1.15.0",
		"@typescript-eslint/eslint-plugin": "^5.43.0",
		"@typescript-eslint/parser": "^5.43.0",
		"autoprefixer": "^10.4.13",
		"babel-jest": "^29.3.1",
		"eslint": "^8.27.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-storybook": "^0.6.7",
		"eslint-plugin-svelte3": "^4.0.0",
		"jest": "^29.3.1",
		"postcss": "^8.4.19",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.8.1",
		"prettier-plugin-tailwindcss": "^0.2.0-beta.2",
		"prisma": "^4.6.1",
		"storybook-dark-mode": "^1.1.2",
		"svelte": "^3.53.1",
		"svelte-check": "^2.9.2",
		"svelte-dnd-action": "^0.9.22",
		"svelte-jester": "^2.3.2",
		"svelte-loader": "^3.1.4",
		"svelte-popperjs": "^1.3.2",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "3.2.3",
		"ts-jest": "^29.0.3",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.3",
		"vite": "^3.2.4",
		"lucide-svelte": "^0.101.0"
	},
	"type": "module"
}